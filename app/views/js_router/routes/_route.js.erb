function <%= route[:name] %>_path(<%= route[:params].join(', ') %>){
  path = '<%= route[:path] %>';
  <% unless route[:params].empty? %>
    params = [<%= route[:params].collect{|x| "'#{x}'"}.join(', ') %>]    
    for(i=0; i < <%=route[:params].length %>; i++)
    {
      path = path.replace(':'+ params[i], eval(params[i]))
    }
  <% end %>
  return path;
}
